<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
  <link rel="icon" href="https://cdn.discordapp.com/attachments/756985061534072941/777161331874201650/oie_transparent_3.png" style="border-width: 50px;" type="image/x-icon" class="botuser">
  <link rel="stylesheet" href="public/bootstrap.min.css">
  <link rel="stylesheet" href="public/main.css">
  <link rel="stylesheet" href="public/now-ui-kit.css">
  <link href="https://fonts.googleapis.com/css?family=Poppins:300,400,600,700,800,900&display=swap" rel="stylesheet">
  <link href="https://fonts.googleapis.com/css?family=Montserrat:400,600,700,800,900&display=swap" rel="stylesheet">
  <link href="https://fonts.googleapis.com/css?family=Lato:300,400,600,700,800,900&display=swap" rel="stylesheet">
  <link rel="stylesheet" href="public/main.css">
</head>
  
<style>
  @import url("https://cdn.jsdelivr.net/npm/now-ui-kit@1.3.0/assets/css/now-ui-kit.min.css");
  @import url("https://stackpath.bootstrapcdn.com/bootstrap/4.5.2/css/bootstrap.min.css");
  @import url("https://cdn.discordapp.com/attachments/756985061534072941/761970807298850846/main.css");
  @import url("hi-kit.css");
  
@-webkit-keyframes spin {
	0% {
		-webkit-transform: rotate(0deg);
	}
	100% {
		-webkit-transform: rotate(360deg);
	}
}
@keyframes spin {
	0% {
		transform: rotate(0deg);
	}
	100% {
		transform: rotate(360deg);
	}
}
  
::-webkit-scrollbar {
	background: #FFFFFF;
	width: 8px;
}
::-webkit-scrollbar-button {
	background: none;
  display: none;
}
::-webkit-scrollbar-track {
	background: #222222;
}
::-webkit-scrollbar-track-piece {
	background: #23272A;
}
::-webkit-scrollbar-thumb {
	background: #4a4f56;
	border-radius: 10px 10px 10px 10px
}
::-webkit-scrollbar-corner {
	background: #4a4f56;
}
  
  .login-button {
    border: 2px solid #AD91FF !important;
  }
  .login-button:hover {
    border: 2px solid #AD91FF !important;
    background-color: #AD91FF !important;
  }
  .btn-secondarys{
    background-color: transparent !important;
    text-transform: none !important;
  }
  .user-imageZ {
    border-radius: 50%;
    width: 30px;
  }
</style>
  
<body>
<nav class="navbar navbar-expand-lg bg-transparent">
    <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarSupportedContent" aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation">
        <img src="https://cdn.discordapp.com/attachments/756985061534072941/761971794730942535/menuIcon.svg" width="20px" height="20px" style="max-width: none !important;">
    </button>
    <img src="https://cdn.discordapp.com/attachments/756985061534072941/777161331874201650/oie_transparent_3.png" width="50" height="50">
    <div class="collapse navbar-collapse" id="navbarSupportedContent" style="margin-left: 20px !important">
        <ul class="navbar-nav mr-auto">
            <li class="nav-item active">
                <a class="nav-link" href="/">Inicio <span class="sr-only">(current)</span></a>
            </li>
            <li class="nav-item active">
                <a class="nav-link" href="/bots">Bots <span class="sr-only"></span></a>
            </li>
        </ul>
      
        <% if(auth) { %>
        <div class="dropdown">
          <a class="btn btn-secondarys dropdown-toggle" role="button" id="dropdownMenuLink" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false" style="text-transform: none!important;">
           <img src="https://cdn.discordapp.com/avatars/<%= user.id %>/<%= user.avatar %>.jpeg?size=1024" class="user-imageZ"> <%= user.username %>
          </a>
          <div class="dropdown-menu" aria-labelledby="dropdownMenuLink">
            <a class="dropdown-item" href="/add/bot">Adicione um Bot</a>
            <a class="dropdown-item" href="/guide">Docs</a>
            <a class="dropdown-item" href="/me">Dashboard</a>
            <% const ids = db.fetch(`Admins_dashboard.ids`) %>
            <% if(ids.includes(user.id)){ %>
              <a class="dropdown-item" href="/admin">Admin</a>
            <% } %>
          </div>
        </div>
      
      <% } %>
        <% if (auth){ %>
        <a href="/logout" class="btn login-button btn-outline-accent my-2 my-sm-0" style="font-size: 10px !important;font-family: poppins !important;color: white;">SAIR</a>
        <% } else { %>
        <a href="/login" class="btn login-button btn-outline-accent my-2 my-sm-0" style="font-size: 10px !important;font-family: poppins !important;color: white;">LOGIN</a>
        <% } %>
    </div>
</nav>
</body>
  <script src="https://code.jquery.com/jquery-3.5.1.slim.min.js" integrity="sha384-DfXdz2htPH0lsSSs5nCTpuj/zy4C+OGpamoFVy38MVBnE+IbbVYUew+OrCXaRkfj" crossorigin="anonymous"></script>
